{
  "name": "QoLBDV",
  "version": "1.0",
  "permissions": [
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "favicon2.png",
    "default_title": "QoLBDV"
  },
  "commands": {
    "toggle-feature-foo": {
      "suggested_key": {
        "default": "Ctrl+Comma"
      },
      "description": "Open Index Page"
    }
  }
}
